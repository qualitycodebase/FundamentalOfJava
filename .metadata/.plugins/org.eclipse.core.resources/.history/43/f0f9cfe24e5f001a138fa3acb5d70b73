package mathOperation;

public class ArthimeticOperation {

	public static void main(String[] args) {
		// + , - , * , / , % (modulus)
		
		int SUM;
		
		int x = 10;
		int y = 203423;
		
		SUM = x + y ;
		
		//System.out.println(SUM);
		
		
	double	SUB;
	
	int xs = 13;
	int ys = 2;
	
	SUB = xs%ys;
	
	System.out.println(SUB);
	
	// 7/2 = 3 & 1/2
	

	

	}

}
